public interface  org.quartz.SchedulerListener extends java.lang.Object
{

    public abstract void jobScheduled(org.quartz.Trigger);

    public abstract void jobUnscheduled(org.quartz.TriggerKey);

    public abstract void triggerFinalized(org.quartz.Trigger);

    public abstract void triggerPaused(org.quartz.TriggerKey);

    public abstract void triggersPaused(java.lang.String);

    public abstract void triggerResumed(org.quartz.TriggerKey);

    public abstract void triggersResumed(java.lang.String);

    public abstract void jobAdded(org.quartz.JobDetail);

    public abstract void jobDeleted(org.quartz.JobKey);

    public abstract void jobPaused(org.quartz.JobKey);

    public abstract void jobsPaused(java.lang.String);

    public abstract void jobResumed(org.quartz.JobKey);

    public abstract void jobsResumed(java.lang.String);

    public abstract void schedulerError(java.lang.String, org.quartz.SchedulerException);

    public abstract void schedulerInStandbyMode();

    public abstract void schedulerStarted();

    public abstract void schedulerStarting();

    public abstract void schedulerShutdown();

    public abstract void schedulerShuttingdown();

    public abstract void schedulingDataCleared();
}
