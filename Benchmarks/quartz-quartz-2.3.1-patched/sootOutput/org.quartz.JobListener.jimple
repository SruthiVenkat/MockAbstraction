public interface  org.quartz.JobListener extends java.lang.Object
{

    public abstract java.lang.String getName();

    public abstract void jobToBeExecuted(org.quartz.JobExecutionContext);

    public abstract void jobExecutionVetoed(org.quartz.JobExecutionContext);

    public abstract void jobWasExecuted(org.quartz.JobExecutionContext, org.quartz.JobExecutionException);
}
